{
	"name": "seven-seed-dashboard",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "eslint . --cache --cache-location .next/cache/eslint --max-warnings=0 && prettier --check '**/*.json'",
		"lint:fix": "eslint . --cache --fix --max-warnings=0 && prettier --write '**/*.json'",
		"format": "prettier --write src/ *.md *.json",
		"typecheck:watch": "tsc --noEmit --watch --preserveWatchOutput --project tsconfig.watch.json"
	},
	"dependencies": {
		"@formspree/react": "^3.0.0",
		"@headlessui/react": "^2.2.9",
		"@heroicons/react": "^2.2.0",
		"@lostisworld/tailwind-mask": "^1.0.1",
		"@tailwindcss/postcss": "^4.1.18",
		"@types/node": "25.2.2",
		"@types/react": "19.2.13",
		"@types/react-dom": "19.2.3",
		"autoprefixer": "10.4.24",
		"clsx": "^2.1.1",
		"eslint-config-next": "^16.1.6",
		"next": "^16.1.6",
		"postcss": "^8.5.6",
		"prettier": "^3.8.1",
		"react": "19.2.4",
		"react-dom": "19.2.4",
		"tailwindcss": "^4.1.18"
	},
	"prettier": {
		"trailingComma": "all",
		"printWidth": 88,
		"useTabs": true,
		"tabWidth": 4,
		"endOfLine": "auto"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"@tailwindcss/typography": "^0.5.19",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.5",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.55.0"
	}
}
